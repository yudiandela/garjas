{
    "name": "yudiandela/garjas",
    "description": "Perhitungan Kesegaran Jasmani A dan B TNI pria dan wanita",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Yudiandela\\Garjas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Yudi Andela",
            "email": "yudhi.andhela@gmail.com"
        }
    ],
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.2",
        "pestphp/pest": "^3.8",
        "friendsofphp/php-cs-fixer": "^3.75"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/pest --colors=always"
        ]
    }
}
